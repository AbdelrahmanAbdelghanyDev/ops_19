    </record>
   <!--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <record id="awe_crm_case_form_view_leads" model="ir.ui.view">
      <field name="name">awe_crm_case_form_view_leads</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="custom_opportunity_cost_estimation_v11.lead_product_form_view"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//page[@name='Products']" position="attributes">
                <attribute name="string">Methodology</attribute>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='product_id']" position="replace">
                <field name="product_id" string="Methodology" domain="[('sale_ok','=',True)]"/>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='category']" position="attributes">
                <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='uom_id']" position="attributes">
                <attribute name="invisible">True</attribute>
          </xpath>
        </data>
      </field>
    </record>

    <record id="awe_crm_case_form_view_opp" model="ir.ui.view">
      <field name="name">awe_crm_case_form_view_opp</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="custom_opportunity_cost_estimation_v11.opportunity_product_form_view"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//page[@name='products']" position="attributes">
              <attribute name="string">Methodology</attribute>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='product_id']" position="after">
                <field name="research_type_id" domain="[('name','in',['QL','QN'])]"/>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='product_id']" position="replace">
                <field name="product_id" string="Methodology" />
          </xpath>
          <xpath expr="//page/field/tree/field[@name='product_id']" position="before">
                <field name="breif" invisible="True"/>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='category']" position="attributes">
                <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='uom_id']" position="attributes">
                <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page/group/group/field[@name='approach']" position="attributes">
                <attribute name="string">Data Capture</attribute>
          </xpath>
          <xpath expr="//page/group/group/field[@name='age']" position="after">
                <field name="project_objective" invisible="True"/>
                <field name="working_country" invisible="True"/>
          </xpath>

           <xpath expr="//page[@name='brief']/group/group/field[@name='number_of_legs']" position="after">

              <!-- <field name="dp" string="DP"/> -->
              <field name="objective" string="Objective" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="methodology" string="Methodology" attrs="{'invisible': [('research_type_name', '!=', 'QN')]}"/>
              <field name="methodology_desc" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}" />
              <field name="criteria_usage" string="Criteria / Usage" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="translation" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="other_translation" string="Other Translation" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>







          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='age']" position="after">
              <field name="research_type_name" invisible="1"/>

              <field name="client_attendance" invisible="True"/>
              <field name="sample_size_text" string="Sample Size" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="sample_struct_per_reg"  attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="sample_struct_per_sec"  attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="sample_struct_per_age"  attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="sample_struct_per_gen"  attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="sample_struct_per_bra" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="no_of_client_attendees" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="no_of_units_attendees" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="client_attendence_region_ids" string=" Client Attendance Region" widget='many2many_tags' attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="viewing_facility" string="Viewing Facility Outside Cairo" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
          </xpath>
          <!--<xpath expr="//page[@name='brief']/group/group/field[@name='usership']" position="after">-->
              <!---->
          <!--</xpath>-->
            <!--mmmmmmmmmmmmmmmmmmmmahfouz-->
          <xpath expr="//page[@name='brief']/group/group" position="after">
              <group>
              <field name="transcript" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="ex_transcript" string="Extended Transcripts" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="transcript_lang" string="Transcripts Language" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="printing_material" string="Printing Material" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="details_ql" attrs="{'invisible': [('research_type_name', '!=', 'QL')],'required': [('research_type_name', '=','QL')]}"/>
              <field name="length_of_interview" invisible="1"/>
              <field name="length_of_interview_sel" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              <field name="details" attrs="{'invisible': [('research_type_name', '!=', 'QN')],'required': [('research_type_name', '=','QN')]}"/>
              </group>


          </xpath>
            <!--mmmmmmmmmmmmmmmmmmmmahfouz-->

          <xpath expr="//page[@name='brief']/group/group/field[@name='research_type']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='approach']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='methodology']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='number_of_legs']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='sample_size']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='gender']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='sec']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='age']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='region']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='moub']" position="attributes">
               <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='brief']/group/group/field[@name='usership']" position="attributes">
