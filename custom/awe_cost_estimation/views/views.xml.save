    <record id="custom_crm_lead_wizard_inherit" model="ir.ui.view">
      <field name="name">custom crm lead wizard inherit</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.view_create_opportunity_simplified"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='partner_id']" position="after">
              <field name="research_type_id" string="Research Type" domain="[('name','in',['QL','QN'])]"/>
          </xpath>
        </data>
      </field>
    </record>
    <!--++++++++++++++++ Original Crm Form View +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

    <record id="custom_crm_lead_teams" model="ir.ui.view">
      <field name="name">custom crm lead teams</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='team_id']" position="after">
              <!-- <field name="executive_team_id" string="Executive Team"
                     widget="selection"/> -->
              <field name="revenue_team_id" string="BU"/>
              <field name="data_capture" string="Data Capture" invisible="1"/>
              <field name="project_objective" string="Project Objective"/>
          </xpath>
          <xpath expr="//field[@name='pdt_line']/tree/field[@name='name']" position="after">
              <field name="fw_country" string="FW Country"/>
              <field name="fw_city_country" string="FW City"/>
              <field name="sec" string="SEC" invisible="True"  />
              <field name="research_type" invisible="True"/>
          </xpath>
          <!--<xpath expr="//field[@name='pdt_line']/tree/field[@name='product_uom_qty']" position="after">-->
              <!--<field name="sample_size" string="SS" invisible="True"/>-->
              <!--<field name="number_of_legs" string="NO of legs" invisible="True"/>-->
              <!--<field name="no_of_respondants" invisible="True"/>-->
              <!--<field name="gender" invisible="True"/>-->
              <!--<field name="age" invisible="True" />-->
              <!--<field name="client_attendance" invisible="True"/>-->
              <!--<field name="no_of_attendees" invisible="True"/>-->
          <!--</xpath>-->
          <xpath expr="//field[@name='pdt_line']/tree/field[@name='product_uom_qty']" position="attributes">
              <attribute name="string">SS</attribute>
          </xpath>
          <xpath expr="//field[@name='tag_ids']" position="after">
              <field name="research_type_id" string="Research type" required="1" domain="[('name','in',['QL','QN'])]"/>
              <field name="project_type" string="Project Type"/>
          </xpath>
          <xpath expr="//field[@name='pdt_line']" position="attributes">
              <attribute name="context">{'product_category_id': research_type_id}</attribute>
          </xpath>
  <!--         <xpath expr="//field[@name='pdt_line']/tree/field[@name='product_id']" position="attributes">
              <attribute name="on_change">get_domain_ids()</attribute>
          </xpath> -->
        </data>
      </field>
    </record>


<!--++++++++++++++++ Original Lead Form View +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
      <record id="custom_crm_lead_teams_2" model="ir.ui.view">
      <field name="name">custom crm lead teams 2</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_case_form_view_leads"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='team_id']" position="after">
              <!-- <field name="executive_team_id" string="Executive Team"
                     widget="selection"/> -->
              <field name="revenue_team_id" string="BU"/>
              <!-- <field name="data_capture" string="Data Capture"/> -->
              <!-- <field name="project_objective" string="Project Objective"/> -->
          </xpath>
          <xpath expr="//field[@name='priority']" position="replace">
              <field name="priority" widget="priority" string="Potentiality"/>
          </xpath>
          <xpath expr="//field[@name='tag_ids']" position="replace">
              <field name="tag_ids" widget="many2many_tags" options="{'color_field': 'color', 'no_create_edit': True}" string="Client Type"/>
          </xpath>
        </data>
      </field>
    </record>

      <record id="custom_view_crm_lead2opportunity_partner" model="ir.ui.view">
      <field name="name">custom view crm lead2opportunity partner</field>
      <field name="model">crm.lead2opportunity.partner</field>
      <field name="inherit_id" ref="crm.view_crm_lead2opportunity_partner"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='team_id']" position="after">
              <field name="executive_team_id" string="Executive Team"
                     widget="selection"/>
              <field name="revenue_team_id" string="Revenue Team"
                     widget="selection"/>
          </xpath>
        </data>
      </field>
    </record>

    <record id="custom_sale_order_teams" model="ir.ui.view">
