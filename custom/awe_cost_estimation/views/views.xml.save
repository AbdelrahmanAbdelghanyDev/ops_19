    <record id="awe_customization" model="ir.ui.view">
      <field name="name">awe customization</field>
      <field name="model">opportunity.cost.estimation</field>
      <field name="inherit_id" ref="custom_opportunity_cost_estimation_v11.opportunity_cost_estimation_form"/>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//page/field/tree/field[@name='product_id']" position="replace">
              <!-- <attribute name="string">Cost Item</attribute>
              <attribute name="domain">[('purchase_ok','=',True)]</attribute> -->
              <field name="product_id" string="Cost Item"/>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='price_unit']" position="attributes">
              <attribute name="string">Unit Cost</attribute>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='price_subtotal']" position="attributes">
              <attribute name="string">Total Cost</attribute>
          </xpath>
          <xpath expr="//page/field/tree/field[@name='tax_id']" position="attributes">
              <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//field[@name='amount_tax']" position="attributes">
              <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//field[@name='amount_total']" position="attributes">
              <attribute name="name">amount_untaxed</attribute>
          </xpath>
          <xpath expr="//label[@for='amount_total']" position="attributes">
              <attribute name="for">amount_untaxed</attribute>
              <attribute name="string">Total</attribute>
          </xpath>
          <xpath expr="//field[@name='amount_untaxed']" position="attributes">
              <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//field[@name='product_uom_qty']" position="attributes">
              <attribute name="sum">sum</attribute>
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='product_id']" position="attributes">
              <attribute name="string">Methodology</attribute>
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='category']" position="attributes">
              <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='uom_id']" position="attributes">
              <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//page[@name='Products']" position="attributes">
              <attribute name="string">Methodology</attribute>
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='product_id']" position="after">
              <field name="breif" invisible="True"/>
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='name']" position="after">
              <field name="fw_country" string="FW City"/>
              <field name="sec" string="SEC"/>
              <!-- <field name="project_objective"/> -->
              <field name="research_type" invisible="True"/>
              <!-- <field name="data_capture"/> -->
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='product_uom_qty']" position="after">
              <field name="sample_size" string="SS" invisible="True"/>
              <!-- <field name="no_of_respondants"/> -->
              <field name="number_of_legs" string="NO of legs" invisible="True"/>
              <field name="gender"/>
              <field name="age"/>
              <field name="client_attendance" invisible="True" />
              <!-- <field name="no_of_attendees"/> -->
          </xpath>
          <xpath expr="//field[@name='pdt_line_view']/tree/field[@name='product_uom_qty']" position="attributes">
              <attribute name="string">SS</attribute>
          </xpath>
          <xpath expr="//field[@name='estimation_type']" position="after">
              <field name="cpi" string="CPI"/>
          </xpath>
          <xpath expr="//form" position="attributes">
              <attribute name="create">false</attribute>
          </xpath>
          <xpath expr="//field[@name='estimation_type']" position="attributes">
              <attribute name="string">Research Type</attribute>
              <attribute name="readonly">True</attribute>
          </xpath>
          <xpath expr="//field[@name='partner_id']" position="attributes">
              <attribute name="invisible">True</attribute>
          </xpath>
          <xpath expr="//field[@name='parent_opportunity']" position="replace">
               <field name="parent_opportunity" string="Source Document"/>
          </xpath>
        </data>
      </field>
    </record>

