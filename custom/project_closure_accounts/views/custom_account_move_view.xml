<odoo>
    <data>
        <record id="custom_account_move_view_form" model="ir.ui.view">
            <field name="name">custom_account_move_view_form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='id']" position="after">
                    <field name="cor" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>