<odoo>
    <data>
        <!-- explicit list view definition -->
        <record model="ir.ui.view" id="time_from_list">
            <field name="name">Time From list</field>
            <field name="model">a.time_from</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                    <field name="seq"/>
                </list>
            </field>
        </record>

        <!-- explicit form view definition -->
        <record model="ir.ui.view" id="time_from_form">
            <field name="name">Time From form</field>
            <field name="model">a.time_from</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name"/>
                            <h1><field name="name"/></h1>
                        </div>
                        <field name="seq"/>
                    </sheet>
                   <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>
        <!-- actions opening views on models -->

        <record model="ir.actions.act_window" id="time_from_action_window">
            <field name="name">Time From </field>
            <field name="res_model">a.time_from</field>
            <field name="view_mode">list,form</field>
        </record>


        <!-- Menu -->

        <menuitem name="Time From" id="time_from_menu_1_list" parent="media_tracking_conf_menu"
                  action="time_from_action_window" sequence="7"/>

    </data>
</odoo>