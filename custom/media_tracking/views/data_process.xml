<odoo>
    <data>
        <!-- explicit list view definition -->
        <record model="ir.ui.view" id="data_processing_list">
            <field name="name">Data Processing list</field>
            <field name="model">a.data.processing</field>
            <field name="arch" type="xml">
                <list create ="0" delete="0">
                    <field name="period"/>
                    <field name="channel_lines" invisible="1"/>
                    <field name="name"/>
                    <field name="respondent_id"/>
                    <field name="header"/>
                    <field name="time_from"  options="{'no_open': True,'no_create':True, 'no_quick_create': True, 'no_create_edit':True}"/>
                    <field name="time_to" options="{'no_open': True,'no_create':True, 'no_quick_create': True, 'no_create_edit':True}"/>
                    <field name="channel_id"/>
                    <field name="programs_id" />
                    <field name="region"/>
                    <field name="age_range"/>
                    <field name="sec"/>
                    <field name="gender"/>
                    <field name="date"/>
                    <field name="day_name"/>
                    <field name="week_end"/>
                    <field name="week_day"/>
                </list>
            </field>
        </record>

        <!-- explicit form view definition -->
        <record model="ir.ui.view" id="data_processing_form">
            <field name="name">Data Processing form</field>
            <field name="model">a.data.processing</field>
            <field name="arch" type="xml">
                <form duplicate="0" create ="0" delete="0">
                    <sheet>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name"/>
                            <h1><field name="name" readonly="1"/></h1>
                        </div>
                        <group>
                            <group>
                                <field name="respondent_id" readonly="1"/>
                                <field name="header" readonly="1"/>
                                <field name="time_from" options="{'no_open': True,'no_create':True, 'no_quick_create': True, 'no_create_edit':True}" readonly="1"/>
                                <field name="time_to"  options="{'no_open': True,'no_create':True, 'no_quick_create': True, 'no_create_edit':True}" readonly="1"/>
                                <field name="channel_id" readonly="1"/>
                            </group>
                            <group>
                                <field name="date" readonly="1"/>
                                <field name="day_name" readonly="1"/>
                                <field name="age_range" readonly="1"/>
                                <field name="sec" readonly="1"/>
                                <field name="programs_id" readonly="1"/>
                                <field name="region" readonly="1"/>
                            </group>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>
        <!-- actions opening views on models -->

        <record model="ir.actions.act_window" id="data_processing_action_window">
            <field name="name">Data Processing </field>
            <field name="res_model">a.data.processing</field>
            <field name="view_mode">list</field>
        </record>


        <!-- Menu -->

        <menuitem name="Data Processing" id="data_processing_menu_1_list" parent="media_tracking_menu"
                  action="data_processing_action_window" sequence="2" groups="media_tracking.group_admin,base.group_system,media_tracking.group_qc,media_tracking.group_user,media_tracking.group_no_approve"/>

    </data>
</odoo>