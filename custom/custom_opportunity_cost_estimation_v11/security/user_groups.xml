<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record model="ir.module.category" id="cost_estimation_module">
      <field name="name">Cost estimation module</field>
      <field name="description">User access level for this module</field>
      <field name="sequence">10</field>
    </record>

    <record id="group_guest" model="res.groups">
      <field name="name">Guest</field>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
      <field name="users" eval="[(4, ref('base.user_root'))]"/>
      <field name="category_id" ref="custom_opportunity_cost_estimation_v11.cost_estimation_module"/>
     </record>

   <record id="group_viewer" model="res.groups">
     <field name="name">Viewer</field>
     <field name="implied_ids" eval="[(4, ref('custom_opportunity_cost_estimation_v11.group_guest'))]"/>
     <!-- <field name="users" eval="[(4, ref('base.user_root'))]"/> -->
     <field name="category_id" ref="custom_opportunity_cost_estimation_v11.cost_estimation_module"/>
    </record>

    <record id="group_user" model="res.groups">
      <field name="name">User</field>
      <field name="implied_ids" eval="[(4, ref('custom_opportunity_cost_estimation_v11.group_viewer'))]"/>
      <field name="category_id" ref="custom_opportunity_cost_estimation_v11.cost_estimation_module"/>
    </record>
  </data>
</odoo>
