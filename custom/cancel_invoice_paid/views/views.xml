<odoo>
  <data>
    <record id="invoice_form_cancel_inherit_paid" model="ir.ui.view">
            <field name="name">invoice.form.cancel.inherit.paid</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='button_cancel']" position="replace">
                        <button name="button_cancel" string="Cancel Entry" type="object" groups="account.group_account_invoice" invisible="[('id', '=', False)]"/>
                </xpath>
            </field>
        </record>
  </data>
</odoo>